			"visible": true,
			"menubar": true
		}
	},
	"plugins": {
		"files": [ "plugins/demo" ]
	},
	"scripts": {
		"files": [ "assets/scripts/setup.py" ]
	},
	
	"testing": {
		"obj0": {
			"type": "lib.class",
			"args": {
				"enabled"	: true,
